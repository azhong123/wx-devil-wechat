/**app.wxss 全局样式**/

.container {
  height: 100%;
  display: flex;
  /*默认列排布  */
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: #fff;
  font-family: "Microsoft Yahei";
}

/*分割线  */
.separate{
  height: 15rpx;
  width: 100%;
  background-color: #fafafa;
}